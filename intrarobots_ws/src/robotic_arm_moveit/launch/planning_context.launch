<launch>
  <!-- By default we do not overwrite the URDF. Change the following to true to change the default behavior -->
  <!-- Setting 'load_robot_description' to 'true' will load the full model including the gripper. Model will not have any color in Gazebo. -->
  <arg name="load_robot_description" default="false"/>
  <arg name="robot_name" default=""/>
  <arg name="robot_model" default=""/>
  <arg name="base_link_frame" default=""/>
  <arg name="show_ar_tag" default=""/>
  <arg name="external_srdf_loc" default=""/>
  <arg name="robot_urdf" default=""/>
  <arg name="robot_srdf" default=""/>
  <arg name="robot_joint_limits_yaml" default=""/>
  <arg name="kinematics_yaml" default=""/>
  <!-- <arg name="sensors_3d_yaml" default=""/> -->

  <!-- The name of the parameter under which the URDF is loaded -->
  <arg name="robot_description" default="robot_description"/>


  <!-- Load universal robot description format (URDF) -->
  <param if="$(arg load_robot_description)" name="$(arg robot_description)" command="xacro $(arg robot_urdf)"/>

  <!-- The semantic description that corresponds to the URDF -->
  <param name="$(arg robot_description)_semantic"

    command="xacro $(arg robot_srdf)
        robot_name:=$(arg robot_name)
        base_link_frame:=$(arg base_link_frame)
        show_ar_tag:=$(arg show_ar_tag)
        external_srdf_loc:=$(arg external_srdf_loc)"/>

  <!-- Load updated joint limits (override information from URDF) -->
  <group ns="$(arg robot_description)_planning">
    <rosparam command="load" file="$(arg robot_joint_limits_yaml)"/>
  </group>

  <!-- Load default settings for kinematics; these settings are overridden by settings in a node's namespace -->
  <group ns="$(arg robot_description)_kinematics">
    <rosparam command="load" file="$(arg kinematics_yaml)"/>
  </group>

</launch>
